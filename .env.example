# Trading RL Agent Environment Variables
# =====================================
# Copy this file to .env and fill in your actual values

# Environment and System Settings
TRADE_AGENT_ENVIRONMENT=development
TRADE_AGENT_DEBUG=true
TRADE_AGENT_LOG_LEVEL=INFO

# Qwen API Configuration
QWEN_API_KEY=your_qwen_api_key_here
QWEN_BASE_URL=https://openrouter.ai/api/v1
QWEN_MODEL=qwen/qwen3-coder:free

# OpenAI-compatible environment variables for qwen CLI
OPENAI_API_KEY=your_qwen_api_key_here
OPENAI_BASE_URL=https://openrouter.ai/api/v1

# Alpaca Trading API Configuration
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# Add other environment variables as needed...
